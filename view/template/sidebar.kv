<Sidebar@MDNavigationDrawer>:
    id: nav_drawer
    NavigationDrawerToolbar:
        title: "Menu Principal"
    NavigationDrawerIconButton:
        icon: 'home'
        text: "Dashboard"
        on_release: app.root.ids.manager.current = 'dashboard'
    NavigationDrawerIconButton:
        icon: 'checkbox-blank-circle'
        text: "Cliente"
        on_release: app.root.ids.manager.current = 'cliente'
    NavigationDrawerIconButton:
        icon: 'checkbox-blank-circle'
        text: "Funcionário"
        on_release: app.root.ids.manager.current = 'funcionario'
    NavigationDrawerIconButton:
        icon: 'checkbox-blank-circle'
        text: "Produto"
        on_release: app.root.ids.manager.current = 'produto'
    NavigationDrawerIconButton:
        icon: 'checkbox-blank-circle'
        text: "Serviço"
        on_release: app.root.ids.manager.current = 'servico'